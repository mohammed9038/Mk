				}
			}
		}
	}
}

@mixin cdztab_style03(){
	.cdztab{
		margin: 0 0px 30px;
		.tab-item{
			list-style: none;
			display: inline-block;
			margin: 0 10px;
			a{
				padding: 5px 10px;
				display: block;
				@include abs-borderradius(0);
				@include h5_font();
				text-decoration: none;
				color: $main_tab_text;
				background: $main_tab_background;
				border: 0px solid $main_tab_border;
				@include transition(all 500ms ease);
				position: relative;
				text-transform: uppercase;
				&:after{
					width: 100%;
					height: 100%;
					border: 1px solid transparent;
					content: "";
					position: absolute;
					top: -2px;
					left: 2px;
					@include transition(all 500ms ease);
					z-index: 1;
				}
			}
			&:hover,
			&.active{
				a{
					border-color: $main_tab_hover_border;
					color: $main_tab_hover_text;
					background: $main_tab_hover_background;	
					&:after{
						border-color: $main_tab_hover_border;
					}
				}
			}
		}
	}
}

