    margin: 0;
    position: relative;
    @include backface();
  }
}

.slide-hide,
.slide-hide > img {
  height: 1px;
}

.slide-hide .slides__image,
.slide-hide .placeholder-svg {
  display: none;
}

.slides__image {
  width: 100%;
  max-width: 100%;
  margin: 0 auto;
  display: block;
  object-fit: cover;
  @include abs-borderradius($main_box_radius);
}

.slides { @include clearfix; }

.slides__text-content {
  width: 80%;
  margin: 0 auto;
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  @include transform(translateY(-50%));
  z-index: 1;

  .slide-hide & {
    display: none;
  }

  .no-js & {
    &::after {
      opacity: 0;
      visibility: hidden;
      content: none;
    }
  }
}

.slides__title,.slides__subtext {
  display: none;
  margin-bottom: 0;
}
.slides__btn {
  display: none;
  border: none;


}

.slides__title + .slides__btn {
  margin-top: 20px;
}


.slides__btn {
  max-width: 100%;
  display: none;
  word-wrap: break-word;
  min-height: 3.125rem;
  line-height: 2.2;


}

.slide-hide .slides__btn {
  display: none;
}


@media screen and (min-width: 769px){
