@mixin cdztab_style01(){
	.cdztab{
		margin: 0 1px 20px;
		.tab-item{
			list-style: none;
			display: inline-block;
			border: 1px solid $main_tab_border;
			margin: 0 -1px;
			a{
				padding: 7px 24px;
				display: block;
				@include abs-borderradius(0);
				@include h3_font();
				font-weight: 500;
				text-decoration: none;
				color: $main_tab_text;
				background: $main_tab_background;
				border-bottom: 3px solid transparent;
			}
			&:hover,
			&.active{
				border-bottom-color: $main_tab_hover_border;
				a{
					border-color: $main_tab_hover_border;
					color: $main_tab_hover_text;
					background: $main_tab_hover_background;	
				}
			}
		}
	}
}
@mixin cdztab_style02(){
	.cdztab{
		margin: 0 0px 40px;
		.tab-item{
			list-style: none;
			display: inline-block;
			border: 0;
			margin: 0 5px;
			a{
