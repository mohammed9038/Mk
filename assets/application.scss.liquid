/*Product Page*/
/*.lSAction>a{ background-image: url('{{ 'controls.png' | asset_url }}');}*/
.rtl-layout {
	.lSAction>.lSPrev{
		left: auto;
		right: 10px;
	}
	.lSAction>.lSNext{
		left: 10px;
		right: auto;
	}
}
.lSAction>a{
	&:before{
		@include abs-iconfont('\f104', 26px);
		border: 1px solid $main_text;
		color: $main_text;
		width: 32px;
		line-height: 32px;
		text-align: center;
		@include abs-borderradius(32px);
		.rtl-layout &{
			content:'\f105';
		}
	}
	&.lSNext:before{
		content:'\f105';
		.rtl-layout &{
			content:'\f104';
		}
	}
	&:hover{
		&:before{
			border-color: $main_hover_text;
			background-color: $main_hover_text;
			color: #ffffff;
		}
	}
}
.vertical{
	.lSAction>a{
		&:before{
			content:'\f106';
		}
	}
	&.lSNext:before{
		content:'\f107';
	}
}
