{% if settings.home_blog_style01 == true %}
	.blogs-style-01{
		.post-item{
			background: $main_box_background;
			height: 100%;	
			@include abs-borderradius($main_box_radius);
		}
		.post-top{position: relative}
		.post-top .post-img{
			overflow: hidden;
			@include abs-borderradius($main_box_radius $main_box_radius 0 0);
			img{
				@include abs-transition();
			}
			&:hover img{
				@include abs-transform(scale(1.1, 1.1) rotate(2deg));
			}
		}
		.post-bottom{padding: 20px}
		.post-category{
			text-transform: uppercase;
			font-weight: 500;
			margin-bottom: 5px;
			display: inline-block;
			vertical-align: top;
			float:left;
			padding: 4px 10px 2px;
			margin-right: 10px;
			position: absolute;
			top: 10px;
			left: 10px;
			color: $main_text_normal2;
			font-size: 85%;
			background-color: $main_hover_text;
			@include abs-borderradius($main_box_radius);
			&:hover{
				color: $main_text_normal2;
			}
			.rtl-layout &{
				left: auto;
				right: 10px;
				margin-right: 0
			}
		}
		.post-title{
			margin-bottom: 10px;
			clear: both;		
			.post-item-link{
				@include h4_font();
				font-weight: 600;
				color: $main_productname_text;
				&:hover{
					color: $main_hover_text;
				}
				
			}
		}
		.post-desc{
			color: $main_text;
			margin-top: 15px;
			margin-bottom: 15px;
			@include h5_font();
			font-weight: 500;
		}
		.post-date{
			color: $main_text_normal;
			@include note_font();
			float: left;
			.rtl-layout &{
				float: right;
			}
			&:before{
				@include abs-iconfont('\f073', 12px);
				margin-right: 8px;
				.rtl-layout &{
					margin-left: 8px;
					margin-right: 0;
				}
			}
		}
		.post-author{
			color: $main_text_normal;
			font-size: 90%;
			text-transform: uppercase;			
			&:before{
				@include abs-iconfont2('\e9aa', 13px);
				margin-right: 8px;
				.rtl-layout &{
					margin-left: 8px;
					margin-right: 0;
				}
			}
		}
		.post-footer{
			border-top: 1px solid $main_border;	
			padding: 20px 0 0;
			margin-top: 10px;
			overflow: hidden;
		}
		.more-link{
			color: $main_text;
			float: right;
			&:hover{
				color: $main_hover_text;
				text-decoration: none;
			}
			.rtl-layout &{
				float: left;
			}
		}	
	}
	@media #{$phone}{
		.blogs-style-01{
			margin-bottom: 0;
			.post-bottom{padding: 15px}	
			.post-title{
				white-space: nowrap;
				text-overflow: ellipsis;
				overflow: hidden;
				
				.post-item-link{
					color: $main_productname_text;
					&:hover{
						text-decoration:none;
					}
				}
