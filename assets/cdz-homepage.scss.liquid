/*================== Slideshow =========================*/
$button: 40px;
.cdz-slideshow{
	ul, ol {
		padding:0;
		margin:0;
		list-style: none;
	}
	.icon-loading{
		font-size: 0 !important;
		span{
			font-size: 0;
		}
	}
	&.cdz-box-slideshow{
		.main-slideshow-inner{
			padding:10px;
			background-color:$main_box_background;
		}
	}
	.slideshow-wrapper{
		position:relative;
	}
	.slick-slide{
		.rtl-layout & {
			float:right;
		}
	}
	.slide-dots-style02{
		.slider__controls-container .slick-dots li.slick-active a:before{
			font-size: 15px;
		}
	}
	.slide-dots-style03{
		.slider__controls-container .slick-dots li{
			a{
				&:before{
					content: "\f111";
					font-size: 12px;
				}
			}
			&.slick-active a:before{
				font-size: 14px;
			}
		}
	}
	.slide-dots-style04{
		.slider__controls-container .slick-dots li{
			a{
				background: transparent;
				border: 2px solid $main_hover_text;
				@include abs-opacity(0.8);
				@include transform(scale(0.8));
				width: 15px;
				height: 15px;
				&:before{
					display: none;
				}
			}
			&.slick-active a{
				background: $main_hover_text;
				border: 2px solid rgba(255,255,255,0.6);
				@include abs-opacity(1);
				@include transform(scale(1));
			}
		}
	}
	.slide-arrow-style02{
		.slider__controls-container .slider__controls-button{
			width: 50px;
			height: 100px;
			line-height: 98px;
			top: -moz-calc((100% - 100px )/2 );
			top: -o-calc((100% - 100px )/2 );
			top: -webkit-calc((100% - 100px )/2 );
			top: calc((100% - 100px )/2 );
			@include abs-borderradius(0);
			@include abs-style(#ffffff, #ffffff, transparent);
			&:before {
				font-size: 40px;
			}
			&:hover{
				@include abs-style($btn_primary_text, $btn_primary_border, $btn_primary_background);
			}
		}
	}
	.slide-arrow-style03{
		.slider__controls-container .slider__controls-button{
			width: 42px;
			height: 42px;
			line-height: 40px;
			top: auto;
			bottom: 40px;
			@include abs-borderradius(0);
			border-width: 2px;
			@include abs-style($main_title_text, $main_title_text, #ffffff);
			&:before {
				content: '\f177';
				line-height: 1;
			}
			&.slider__controls-button--next{
				right: 40px;
				left: auto;
				&:before{
					content: '\f178';				
				}
			}
			&.slider__controls-button--prev{
				right: 100px;
				left: auto;
			}
			&:hover{
				&:before {
					@include abs-style($btn_hover_text, $btn_hover_text, #ffffff);
				}
			}
		}
	}
	.slide-arrow-style04{
		.slider__controls-container .slider__controls-button{
			width: 50px;
			height: 80px;
			line-height: 78px;
			top: -moz-calc((100% - 80px )/2 );
			top: -o-calc((100% - 80px )/2 );
			top: -webkit-calc((100% - 80px )/2 );
			top: calc((100% - 80px )/2 );
			@include abs-borderradius(0);
			@include abs-style(#ffffff, transparent, transparent);
			&:before {
				width: 100%;
				height: 100%;
				position: absolute;
				top: 0px;
				left: 0px;
				z-index: 3;
				line-height: 40px;
				font-size: 18px;
				color: $btn_primary_text;
				background: $btn_primary_background;
				-moz-transform: translate(-2px, 2px);
				-webkit-transform: translate(-2px, 2px);
				-o-transform: translate(-2px, 2px);
				transform: translate(-2px, 2px);
				@include transition(all 500ms ease);
				font-size: 26px;
				line-height: 80px;
			}
			&:after {
				border: 1px solid $btn_primary_border;
				width: 100%;
				height: 100%;
				content: '';
				position: absolute;
				top: 0;
				right: 0;
				z-index: 1;
				@include transition(all 500ms ease);
			}
			&:hover{
				&:before {
					color: $btn_hover_text;
					background: $btn_hover_background;
					-moz-transform: translate(0, 0);
					-webkit-transform: translate(0, 0);
					-o-transform: translate(0, 0);
					transform: translate(0, 0);
				}
				&:after {
					border-color: $btn_hover_border;
				}
			}
		}
	}
	.slide-arrow-style05{
		.slider__controls-container .slider__controls-button{
			width: 50px;
			height: 50px;
			line-height: 48px;
			top: -moz-calc((100% - 50px )/2 );
			top: -o-calc((100% - 50px )/2 );
			top: -webkit-calc((100% - 50px )/2 );
			top: calc((100% - 50px )/2 );
			@include abs-borderradius(0);
			&.slider__controls-button--prev{left: 10px;}
			&.slider__controls-button--next{right: 10px;}
		}
	}
	.slider__controls-container{
		position: static;
		.slider__controls-button{
			text-align: center;
			font-size: 0;
			border-width: 1px;
			border-style: solid;
			display: inline-block;
			position: absolute;
			width: $button;
			height: $button;
			line-height: 38px;
			-moz-backface-visibility: hidden;
			-webkit-backface-visibility: hidden;
			backface-visibility: hidden;
			@include abs-style($btn_primary_text, $btn_primary_border, $btn_primary_background);
			top: -moz-calc((100% - #{$button}) / 2 );
			top: -webkit-calc((100% - #{$button}) / 2 );
			top: -o-calc((100% - #{$button}) / 2 );
			top: calc((100% - #{$button}) / 2 );
			@include abs-opacity(0);
			@include transition(all 500ms ease);
			@include abs-borderradius(100%);
			z-index:2;
			padding:0;
			span{display:none;}
			&:before{
				@include abs-iconfont('\f104', 20px);
				line-height:38px;
			}
			&.slider__controls-button--prev{
				left: 40px;
				@include transform(translate(-40px));
				&:hover{
					@include abs-style($btn_primary_background, $btn_primary_background, $btn_primary_text);
				}
			}
			&.slider__controls-button--next{
				right: 40px;
				@include transform(translate($button));
				&:hover{
