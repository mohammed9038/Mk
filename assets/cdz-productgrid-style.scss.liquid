/*================ Global | Sass Mixins ================*/
@mixin clearfix() {
  &:after {
    content: "";
    display: table;
    clear: both; }
  *zoom: 1;
}

@mixin prefix($property, $value) {
  -webkit-#{$property}: #{$value};
  -moz-#{$property}: #{$value};
  -ms-#{$property}: #{$value};
  -o-#{$property}: #{$value};
  #{$property}: #{$value};
}
/*=================== Function ======================*/
@mixin default_font(){
	font: $defaultFont;
}
@mixin h1_font(){
	font: $h1Font;
	color: $main_title_text;
}
@mixin h2_font(){
	font: $h2Font;
	color: $main_title_text;
}
@mixin h3_font(){
	font: $h3Font;
	color: $main_title_text;
}
@mixin h4_font(){
	font: $h4Font;
}
@mixin h5_font(){
	font: $h5Font;
}
@mixin h6_font(){
	font: $h6Font;
}

@mixin prefixFlex($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else if $prefix == moz {
      -moz-#{$property}: $value;
    } @else if $prefix == ms {
      -ms-#{$property}: $value;
    } @else if $prefix == o {
      -o-#{$property}: $value;
    } @else if $prefix == spec {
      #{$property}: $value;
    } @else  {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}

@mixin backface($visibility: hidden) {
  @include prefix('backface-visibility', #{$visibility});
  -moz-backface-visibility: hidden;
	-webkit-backface-visibility: hidden;
	backface-visibility: hidden;
}

@mixin visuallyHidden {
  clip: rect(0 0 0 0);
  clip: rect(0, 0, 0, 0);
  overflow: hidden;
  position: absolute;
  height: 1px;
  width: 1px;
}

@mixin transition($transition: 0.1s all) {
  @include prefix('transition', #{$transition});
}

@mixin transform($transform: 0.1s all) {
  @include prefix('transform', #{$transform});
}

@mixin abs-iconfont($font, $font-size){
	display: inline-block;
	font-family: FontAwesome;
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	vertical-align: middle;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;	
	content:$font;
	font-size:$font-size;
	vertical-align: middle;
}
@mixin abs-iconfont2($font, $font-size){
	display: inline-block;
	font-family: 'CodazonFont';
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;	
	content:$font;
	font-size:$font-size;	
    speak: none;
}

@mixin abs-opacity($opacity) {
	-webkit-opacity: $opacity;
	-moz-opacity: $opacity;
	opacity: $opacity; 
}

@mixin abs-borderradius($value){
	-webkit-border-radius: $value;
	-moz-border-radius: $value;
	border-radius: $value;
}


@mixin abs-boxshadow($value){
  box-shadow: $value;
  -moz-box-shadow: $value;
  -webkit-box-shadow: $value;
  -o-box-shadow: $value;
  -ms-box-shadow: $value;
}

@mixin abs-colorfont( $color, $font){
	color: $color;
	font: $font;
}
@mixin abs-style( $color, $border, $background){
	color: $color;
	border-color: $border;
	background: $background;
}
@mixin abs-allstyle( $color, $border, $background, $font){
	color: $color;
	border-color: $border;
	background: $background;
	font: $font;
}
