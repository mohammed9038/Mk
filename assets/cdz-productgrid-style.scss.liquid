				margin-right: 5px;
				@include abs-borderradius($main_box_radius);
				&:after{display: none;}
				&:before{margin:0;}
			}
			.product-miniature .actions-secondary{
				.action.towishlist, .action.tocompare{
					width: 30px;
					height: 30px;
					line-height: 29px;
					background: $btn_primary_background!important;
					color: $btn_primary_text!important;
					border: 1px solid $btn_primary_border;
					padding: 0;
					text-align: center;
					margin-right: 5px;
					animation: none!important;
					-o-animation: none!important;
					-ms-animation: none!important;
					-webkit-animation: none!important;
					-moz-animation: none!important;
					@include abs-borderradius($main_box_radius);
					&:before{font-size: 12px;}
				}
			}
			.product-miniature .actions-secondary{float: left;}
			.product-miniature .product-item-inner{display: inline-block;}
		}
		&.product-grid-style-02,
		&.product-grid-style-03{
			.product-miniature .actions-primary .btn-cart{font-size: 0;}
		}
		&.product-grid-style-05{
			.product-miniature .product-item-actions{
				width: auto;
				text-align: inherit;
				.actions-primary {				
					float: left;
					margin-right: 5px;
					.btn-cart{
						min-width: 1px;
						margin: 0;
						font-size: 0;
						width: 30px;
						height: 30px;
						line-height: 29px;
						text-align: center;
						padding: 0;
						&:before {
							@include abs-iconfont2('\e992', 16px);
							line-height: 27px;
						}
					}
				}
				.action{
					margin-top: 0;
					width: 30px;
					height: 30px;
					line-height: 29px;
				}
			}
		}
		&.product-grid-style-07{
			.product-miniature .button-hover-wrap .button-panel{display: none;}
			.product-miniature .product-item-bottom-inner .product-item-actions{display: inline-block;}
		}
		&.product-grid-style-10{
			.product-miniature .qty-box{
				margin:0;
				input.qty{width: 70px;}
			}
		}
		&.product-grid-style-12{
			.product-miniature {
				.action{
					width: 30px;
					height: 30px;
					line-height: 29px;
				}
				.product-item-bottom-inner .product-item-actions{
					margin: 0 -5px;
				}
				.actions-primary .btn-cart{
					width: 30px;
					height: 30px;
					line-height: 29px;
					&:before{
						line-height: 29px;
					}
				}
			}
		}
		&.products.product-grid-style-13{
			.product-miniature{
				.actions-primary{
					display: inline-block;
					float: left;
					margin: 0 5px;
					.btn-cart{
						width: 30px;
						height: 30px;
						line-height: 29px;
						text-align: center;
						font-size: 0;
						padding: 0;
						span{
							padding: 0;
							display: inline;
							&:before{
								font-size: 16px;
								line-height: 27px;
								position: static;
								-moz-transform: translate(0);
								-webkit-transform: translate(0);
								-o-transform: translate(0);
								transform: translate(0);
								-webkit-opacity: 1;
								-moz-opacity: 1;
								opacity: 1;
							}
							&:after{display: none;}
						}
					}
				}
				.action{
					width: 30px;
					height: 30px;
					line-height: 29px;
				}
				.product-item-actions {display: inline-block;}
				.cdz-product-deal{
				  .clockdiv{
					  font-size: 14px;
					  > div {
						  .smalltext{
							font-size: 12px;
						  }
						  &:after {
							font-size: 16px;
						  }
					  }
				  }
			  }
			}
		}
	}
	.product-item .qs-button{
		display: none!important;
	}
}
