/*Product hover slide image effect*/
@mixin abs-product-hover-img-slide(){
	.product-image-photo{
		@include transition(all 500ms ease);
		&.hovered-img{
			@include abs-opacity(0);
		}
		&.hovered-img{
			@include abs-transform(translateX(100%));
			.rtl-layout &{
				@include abs-transform(translateX(-100%));
			}
		}
	}
	&:hover{
		.product-image-photo{
			&.main-img{
				@include abs-transform(translateX(-100%));
				@include abs-opacity(0);
				.rtl-layout &{
					@include abs-transform(translateX(100%));
				}
			}
			&.hovered-img{
				@include abs-opacity(1);
				@include abs-transform(translateX(0));
				.rtl-layout &{
					@include abs-transform(translateX(0));
				}
			}
		}
	}
}
