				}
				&:hover{
					background: $btn_primary_text;
					color: $btn_primary_background;
				}
			}
		}
		.action{
			width: 35px;
			height: 35px;
			line-height: 33px;
			float:left;
			margin:0 5px;
			text-align: center;
			background: $btn_primary_background;
			color: $btn_primary_text;
			border: 1px solid $btn_primary_border;
			border-bottom-width:3px;
			border-bottom-color: $border-bottom-color-darken;
			font-size: 0;
			@include abs-borderradius($main_box_radius);
			&:before{
				@include abs-iconfont("\f0ec", 14px);
			}
			&.is-actived, &.is-actived:hover{
				color: #fc7070;
				background: #ffffff;
				border-color: #fc7070;
			}
			&:hover{
				background: $btn_primary_text;
				color: $btn_primary_background;
				border-bottom-color: $border-bottom-color-darken;
			}
			&.towishlist{
				top: -2px;
				position: absolute;
				right: 0;
				background: none;
				color: $main_text_normal;
				border: none;
				margin: 0;
				&:hover{
					color: $main_hover_text; 
				}
				&:before{@include abs-iconfont('\f004', 14px)}
			}
		}
	}
	&.product-items {
		margin: 0;
		.item {
			margin: 0;
			width: 100%;
		}
	}
}
@media only screen and (min-width: 768px){
	.product-list-style-03{
		.product-item-bottom-inner{
			position: absolute;
			right:0;
			left:0;
			bottom: 0;
			z-index: 1; 
			@include transition(all 500ms ease); 
			@include abs-opacity(0); 
			@include transform(scale(0));
		}
		.item:hover  .product-item-bottom-inner {
			@include transform(scale(1));
			@include abs-opacity(1);
		}
	}
}
