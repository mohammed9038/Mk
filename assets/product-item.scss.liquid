/*============== Swatch Option ===================*/
.product-option{
	padding-bottom:5px;
	.swatch{
		input[type="radio"]{
			display:none;
		}
		.swatch-element{
		  display:inline-block;
		  margin-right: 5px;
		  &.disable{
			  label{
				  cursor: no-drop!important;
				  &:before{
					 background: linear-gradient(to left top, rgba(255,255,255,0) 0%, rgba(255,255,255,0) 42%, #fff 45%, #fff 46%, #ff5216 49%, #ff5216 49%, #fff 51%, #fff 52%, rgba(255,255,255,0) 55%, rgba(255,255,255,0) 100%)!important;
					 left:0!important;
					 top:0!important;
					 width: 100%;
					 height: 100%;
					 position: absolute;
					 content: ""!important;
				  }
			  }
		  }
		  label{
			  margin:0;
			  display:block;
			  position: relative;
			  overflow: hidden;
			  cursor: pointer;
		  }
		}
		&.swatch-color{
		  .swatch-element label{
			width: 20px;
			height: 20px;
			overflow: hidden;
			display: block;
		  }
		}
		&.swatch-text{
		   .swatch-element {
			   label{
				display: block;
				border:1px solid #e9eaec;
				background:#fff;
				font-size:12px;
				padding: 2px 6px;
				min-width: 22px;
				//@include abs-borderradius(30px);
			   }
				&:hover label{
					cursor: pointer;
				}
				&.selected label, &:hover label{
					border-color: $btn_primary_border;
					background: $btn_primary_background;
					color: $btn_primary_text
				}
				 &.disable:hover label{
					border-color: #e9eaec!important;
					background: #fff!important;
					color: $main_text!important;
				}
		   }
		}
	}
	.selector-wrapper{
		input[type="radio"] {
			display: none;
		}
	}
}
.swatch{
	.swatch-element{
		font-size:100%;
		label{
			font-size:100%;
		}
	}
	&.swatch-image{
		.swatch-element{
			//@include abs-borderradius(100%);
			font-size:0;
			label{
				//@include abs-borderradius(100%);
				font-size:0;
				position:relative;
				display:block;
				width: 22px;
				height: 22px;
				border: 1px solid transparent;
				box-shadow: 1px 1px 4px #dfdfdf;
				text-indent: -999em;
				text-align: center;
				a{
					width: 100%; height: 100%;
				}
				img{
					height: 100%;
				}
			}
			&.selected label{
				&:after{
					@include abs-iconfont("\f00c", 12px);
					text-shadow: 1px 1px 3px #a0a0a0;
					position: absolute;
					left: 4px;
					top: 4px;
					color:#fff;
				 }
				 &:before{
					background-color: #0000003d; 
					position: absolute;
					left: 0px;
					top: 0px;
					width: 100%;
					height: 100%;
					content: "";
				 }
			}
		}
	}
	&.swatch-color{
		.swatch-element{
			//@include abs-borderradius(100%);
			font-size:0;
			label{
				//@include abs-borderradius(100%);
				font-size:0;
				position:relative;
				display:block;
				width: 20px;
				height: 20px;
				border: 1px solid transparent;
				box-shadow: 1px 1px 4px #dfdfdf;
				text-indent: -999em;
				a{
					width: 100%; height: 100%;
				}
			}
			&.selected label{
				&:before{
					@include abs-iconfont("\f00c", 12px);
					text-shadow: 1px 1px 3px #a0a0a0;
					position: absolute;
					left: 3px;
					top: 3px;
					color:#fff;
				 }
			}
		}
	}
}
