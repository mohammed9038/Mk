{%- if settings.social_twitter_link != blank or settings.social_facebook_link != blank or settings.social_pinterest_link != blank or settings.social_google_plus_link != blank or settings.social_instagram_link != blank or settings.social_snapchat_link != blank or settings.social_tumblr_link != blank or settings.social_youtube_link != blank or settings.social_vimeo_link != blank or settings.social_fancy_link != blank or settings.social_rss_link != blank -%}
      {%- assign show_social_icons = true -%}
    {%- else -%}
      {%- assign show_social_icons = false -%}
{%- endif -%}
{%- assign enable_full_width = section.settings.enable_full_width -%} 
{%- assign enable_full_container = section.settings.enable_full_container -%} 
{%- assign enable_full_background = section.settings.enable_full_background -%}
{%- assign fsocial_title = section.settings.fsocial_title -%} 
{%- assign fcopyright = section.settings.fcopyright -%} 
{%- assign fpayment = section.settings.fpayment -%} 
{%- assign fappstore = section.settings.fappstore -%}
{%- assign footer_background = section.settings.footer_background -%}
{%- assign footer_background_image = section.settings.footer_background_image -%}
{%- assign footer_background_image_option = section.settings.footer_background_image_option -%}
{%- assign footer_top_background = section.settings.footer_top_background -%}
{%- assign footer_bottom_background = section.settings.footer_bottom_background -%}
{%- assign footer_text = section.settings.footer_text -%}
{%- assign footer_text_size = section.settings.footer_text_size -%}
{%- assign footer_title_text_size = section.settings.footer_title_text_size -%}
{%- assign footer_title_text = section.settings.footer_title_text -%}
{%- assign footer_hover_text = section.settings.footer_hover_text -%}
{%- assign footer_border = section.settings.footer_border -%}
{%- assign footer_shadow = section.settings.footer_shadow -%}
{%- assign social_text = section.settings.social_text -%}
{%- assign social_background = section.settings.social_background -%}
{%- assign social_hover_text = section.settings.social_hover_text -%}
{%- assign social_hover_background = section.settings.social_hover_background -%}
{%- assign social_font_size = section.settings.social_font_size -%}
{%- assign social_size = section.settings.social_size -%}
{%- assign newsletter_input_text = section.settings.newsletter_input_text -%}
{%- assign newsletter_input_background = section.settings.newsletter_input_background -%}
{%- assign newsletter_button_text = section.settings.newsletter_button_text -%}
{%- assign newsletter_button_background = section.settings.newsletter_button_background -%}
{%- assign newsletter_button_hover_text = section.settings.newsletter_button_hover_text -%}
{%- assign newsletter_button_hover_background = section.settings.newsletter_button_hover_background -%}
{%- assign main_box_radius = settings.main_box_radius -%}
{%- assign btn_radius = settings.btn_radius -%}

<footer class="site-footer small--text-center" role="contentinfo">
	<div class="footer footer-style-03">
	  	<div class="{% if enable_full_width == false %}container{% endif %}">
			<div class="footer-wrapper">
				<div class="footer-content-top">
					<div class="container">
						<div class="row">
							<div class="col-12">
							{% for block in section.blocks %}
								{% assign footer_block_title = block.settings.footer_block_title %}
								{% assign newsletter_position = block.settings.newsletter_position %}
								{% assign fcustom_block_alignment = block.settings.fcustom_block_alignment %}
								{% assign fcustom_block = block.settings.fcustom_block %}
								{% assign fcustom_block_under = block.settings.fcustom_block_under %}
								{% assign show_social = block.settings.show_social %}
								{% assign show_payment = block.settings.show_payment %}
								{% assign show_appstore = block.settings.show_appstore %}
								{% assign show_copyright = block.settings.show_copyright %}
								{% case block.type %}
									{% when 'newsletter' %}
										{% if newsletter_position == 'top' %}
											{% style %}
												.footer-content-top{
													border-bottom: 1px solid {{footer_border}};
												}
											{% endstyle %}
											<div class="footer-alignment-{{fcustom_block_alignment}}">
												<div class="footer-box">
													{% unless footer_block_title == blank %}
														<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
													{% endunless %}
													<div id="footer-{{block.id}}" class="footer-box-wrapper">
														{% unless fcustom_block == blank %}
														<div class="block-html">
															{{fcustom_block}}
														</div>
														{% endunless %}
														<div class="footer-newsletter">
															{% include 'newsletter-form' %}
														</div>
														{% unless fcustom_block_under == blank %}
														<div class="block-html">
															{{fcustom_block_under}}
														</div>
														{% endunless %}
														{% if show_social %}
															<div class="footer-social">
																{% unless fsocial_title == blank %}
																	<p class="h5" data-action="mb-title-toggle">{{ fsocial_title | escape }}</p>
																{% endunless %}
																{% include 'social-links' %}
															</div>
														{% endif %}
														{% if show_payment %}
															<div class="footer-payment">
																{{fpayment}}
															</div>
														{% endif %}
														{% if show_appstore %}
															<div class="footer-appstore">
																{{fappstore}}
															</div>
														{% endif %}
														{% if show_copyright %}
															<div class="footer-address">
																{{fcopyright}}
															</div>
														{% endif %}
														
													</div>
												</div>
											</div>
										{% endif %}
								{% endcase %}
							{% endfor %}
							</div>
						</div>
					</div>
				</div>
				<div class="footer-content-middle">
					<div class="container">
						<div class="row">
								{% for block in section.blocks %}
								{% assign footer_block_width = block.settings.footer_block_width %}
								{% assign footer_block_tablet_width = block.settings.footer_block_tablet_width %}
								{% assign footer_block_title = block.settings.footer_block_title %}
								{% assign footer_block_sub_title = block.settings.footer_block_sub_title %}
								{% assign newsletter_position = block.settings.newsletter_position %}
								{% assign fcustom_block_alignment = block.settings.fcustom_block_alignment %}
								{% assign fcustom_block = block.settings.fcustom_block %}
								{% assign fcustom_block_under = block.settings.fcustom_block_under %}
								{% assign show_social = block.settings.show_social %}
								{% assign show_payment = block.settings.show_payment %}
								{% assign show_appstore = block.settings.show_appstore %}
								{% assign show_copyright = block.settings.show_copyright %}
								{% case block.type %}
									{% when 'about-us' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% assign fcustom_block_logo = block.settings.fcustom_block_logo %}
													{% comment ls-rpl %}{% assign sizeimg_url = block.settings.fcustom_block_logo | img_url : block.settings.fcustom_block_logo_size %}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: block.settings.fcustom_block_logo, img_url : block.settings.fcustom_block_logo_size -%}{%- endcapture -%}{% assign sizeimg_url = LS_IMG_URL %}{% comment /ls-rpl %}{% endcomment %}
													{% unless fcustom_block_logo == blank %}<div class="footer-logo"><img class="img-responsive" src="{{sizeimg_url}}" /></div>{% endunless %}
													{% unless fcustom_block == blank %}
													<div class="block-html">
														{{fcustom_block}}
													</div>
													{% endunless %}
													{% if show_social %}
														<div class="footer-social">
															{% unless fsocial_title == blank %}
																<p class="h5" data-action="mb-title-toggle">{{ fsocial_title | escape }}</p>
															{% endunless %}
															{% include 'social-links' %}
														</div>
													{% endif %}
													{% if show_payment %}
														<div class="footer-payment">
															{{fpayment}}
														</div>
													{% endif %}
													{% if show_appstore %}
														<div class="footer-appstore">
															{{fappstore}}
														</div>
													{% endif %}
													{% if show_copyright %}
														<div class="footer-address">
															{{fcopyright}}
														</div>
													{% endif %}
												</div>
											</div>
										</div>
									{% when 'custom-block' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% unless fcustom_block == blank %}
													<div class="block-html">
														{{fcustom_block}}
													</div>
													{% endunless %}
												</div>
											</div>
										</div>
									{% when 'link-list' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% unless fcustom_block == blank %}
													<div class="block-html">
														{{fcustom_block}}
													</div>
													{% endunless %}
													<ul class="footer-link">
													  {% for link in linklists[block.settings.footer_block_link_list].links %}
														<li {% if forloop.last == true %} class="last"{% endif %}><a href="{{ link.url }}">{{ link.title }}</a></li>
													  {% endfor %}
													</ul>
													{% unless fcustom_block_under == blank %}
													<div class="block-html">
														{{fcustom_block_under}}
													</div>
													{% endunless %}
													
												</div>
											</div>
										</div>
									{% when 'newsletter' %}
										{% if newsletter_position == 'middle' %}
											<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
												<div class="footer-box">
													{% unless footer_block_title == blank %}
														<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
													{% endunless %}
													<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
														{% unless fcustom_block == blank %}
														<div class="block-html">
															{{fcustom_block}}
														</div>
														{% endunless %}
														<div class="footer-newsletter">
															{% include 'newsletter-form' %}
														</div>
														{% unless fcustom_block_under == blank %}
														<div class="block-html">
															{{fcustom_block_under}}
														</div>
														{% endunless %}
														{% if show_social %}
															<div class="footer-social">
																{% unless fsocial_title == blank %}
																	<p class="h5" data-action="mb-title-toggle">{{ fsocial_title | escape }}</p>
																{% endunless %}
																{% include 'social-links' %}
															</div>
														{% endif %}
														{% if show_payment %}
															<div class="footer-payment">
																{{fpayment}}
															</div>
														{% endif %}
														{% if show_appstore %}
															<div class="footer-appstore">
																{{fappstore}}
															</div>
														{% endif %}
														{% if show_copyright %}
															<div class="footer-address">
																{{fcopyright}}
															</div>
														{% endif %}
														
													</div>
												</div>
											</div>
										{% endif %}
									{% when 'instagram' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% unless block.settings.footer_block_sub_title == blank %}
														<p class="fsub_text">{{block.settings.footer_block_sub_title}}</p>
													{% endunless %}
													<div class="footer-instagram">
														{% include 'grid_style' with id: block.id, show_slider, items_1900, items_1600, items_1440, items_1280, items_980, items_768, items_480, items_320, items_0, items_margin %}
														{% include 'cdz-instagram' %}
													</div>
												</div>
											</div>
										</div>
									{% when 'twitter' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% unless block.settings.footer_block_sub_title == blank %}
													<p class="fsub_text">{{block.settings.footer_block_sub_title}}</p>
													{% endunless %}
													<div class="footer-twitter">
														{% include 'cdz-twitter' %}
													</div>
												</div>
											</div>
										</div>
									{% when 'facebook' %}
										<div class="footer-col footer-alignment-{{fcustom_block_alignment}} cdz-width-{{footer_block_width}} cdz-tablet-width-{{footer_block_tablet_width}}">
											<div class="footer-box">
												{% unless footer_block_title == blank %}
													<p class="h5" data-action="mb-title-toggle">{{ footer_block_title | escape }}</p>
												{% endunless %}
												<div id="footer-{{block.id}}" class="footer-box-wrapper {% unless footer_block_title == blank %}showhide{% endunless %}">
													{% unless block.settings.footer_block_sub_title == blank %}
													<p class="fsub_text">{{block.settings.footer_block_sub_title}}</p>
													{% endunless %}
													<div class="footer-facebook">
														{% include 'cdz-facebook' %}
													</div>
												</div>
											</div>
										</div>
								{% endcase %}
							{% endfor %}
						</div>
					</div>
				</div>
				<div class="footer-content-bottom">
					{% assign fbottom_show_payment = section.settings.fbottom_show_payment %}
					{% assign fbottom_show_appstore = section.settings.fbottom_show_appstore %}
					{% assign fbottom_show_copyright = section.settings.fbottom_show_copyright %}
					{% style %}
						{% if fbottom_show_payment or fbottom_show_appstore or fbottom_show_copyright %}
						.footer-content-bottom{
							border-top: 1px solid {{footer_border}};
						}
						{% endif %}
					{% endstyle %}
					<div class="container">
						<div class="row">
							
							<div class="col-12 col-sm-6 footer-bottom-left">
								{% if fbottom_show_copyright %}
									<div class="footer-address">
										{{fcopyright}}
									</div>
								{% endif %}
							</div>
							<div class="col-12 col-sm-6 footer-bottom-right">
								{% if fbottom_show_payment %}
									<div class="footer-payment">
										{{fpayment}}
									</div>
								{% endif %}
								{% if fbottom_show_appstore %}
									<div class="footer-appstore">
										{{fappstore}}
									</div>
								{% endif %}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</footer>



{% style %}
	.footer{
		clear: both;
		color: {{footer_text}};
		font-size: {{footer_text_size}}px;
		{% if enable_full_width %}
			background: {{footer_background}};
			{% unless footer_background_image == blank %}
			background-image: url({% comment ls-rpl %}{{footer_background_image | img_url: 'master' }}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: footer_background_image, img_url: 'master' -%}{%- endcapture -%}{{LS_IMG_URL }}{% comment /ls-rpl %}{% endcomment %});
			{% endunless %}
			background-repeat: no-repeat;
			background-position: {{footer_background_image_option}};
		{% else %}
			padding-bottom: 30px;
		{% endif %}
		
	}
	.footer-wrapper{
		{% if enable_full_width == false %}
			background: {{footer_background}};
			{% unless footer_background_image == blank %}
			background-image: url({% comment ls-rpl %}{{footer_background_image | img_url: 'master' }}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: footer_background_image, img_url: 'master' -%}{%- endcapture -%}{{LS_IMG_URL }}{% comment /ls-rpl %}{% endcomment %});
			{% endunless %}
			background-repeat: no-repeat;
			background-position: {{footer_background_image_option}};
			-webkit-border-radius: {{main_box_radius}};
			-moz-border-radius: {{main_box_radius}};
			border-radius: {{main_box_radius}};
			box-shadow: {{footer_shadow}};
			-moz-box-shadow: {{footer_shadow}};
			-webkit-box-shadow: {{footer_shadow}};
			-o-box-shadow: {{footer_shadow}};
			-ms-box-shadow: {{footer_shadow}};
		{% endif %}
		
	}
	.footer-alignment-left{
		text-align: left;
	}
	.footer-alignment-right{
		text-align: right;
	}
	.rtl-layout .footer-alignment-left{
		text-align: right;
	}
	.rtl-layout .footer-alignment-right{
		text-align: left;
	}
	.footer-alignment-center{
		text-align: center;
	}
	.footer-content-top{
		background-color: {{footer_top_background}};
		-webkit-border-radius: {{main_box_radius}} {{main_box_radius}} 0 0;
		-moz-border-radius: {{main_box_radius}} {{main_box_radius}} 0 0;
		border-radius: {{main_box_radius}} {{main_box_radius}} 0 0;
	}
	.footer-content-bottom{
		background-color: {{footer_bottom_background}};
		-webkit-border-radius: 0 0 {{main_box_radius}} {{main_box_radius}};
		-moz-border-radius: 0 0 {{main_box_radius}} {{main_box_radius}};
		border-radius: 0 0 {{main_box_radius}} {{main_box_radius}};
	}
	.footer-content-top,
	.footer-content-middle,
	.footer-content-bottom{
		{% if enable_full_width %}
			padding: 10px 0;
		{% else %}
			padding: 10px;
		{% endif %}
	}
	
	.footer-box{
		{% if enable_full_width %}
			padding: 20px 0;
		{% else %}
			padding: 20px 0;
		{% endif %}
	}
	
	.footer a{
		color: {{footer_text}};
		
	}
	.footer a:hover{
		color: {{footer_hover_text}};
	}
	.footer .h5{
		color: {{footer_title_text}};
		text-transform: uppercase;
		margin-bottom: 15px;
		font-size: {{footer_title_text_size}}px;
	}
	
	.footer-link{
		list-style: none;
		padding: 0;
		margin-bottom: 0;
	}
	.footer-link li{
		padding: 5px 0;
	}
	.footer-content-top{
		text-align: center;
	}
	
	.footer-box-wrapper > div ~ div{
		padding-top: 15px;
	}
	
	.footer-newsletter .input-group{
		background: {{newsletter_input_background}};
		position: relative;
		flex-direction: row;
		flex-wrap: nowrap;
		border-bottom: 1px solid {{newsletter_input_text}};
	}
	.footer-newsletter .input-group:before{
		color: {{newsletter_input_text}};
		display: inline-block;
		font-family: FontAwesome;
		font-style: normal;
		font-weight: normal;
		line-height: 1;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
		content: "\f003";
		font-size: 15px;
		vertical-align: middle;
		line-height: 40px;
		width: 35px;
		position: absolute;
		left: 0;
		top: 0;
		text-align: left;
	}
	.rtl-layout .footer-newsletter .input-group:before{
		left: auto;
		right: 0;
		text-align: right;
	}
	.footer-newsletter input[type="email"]{
		background: {{newsletter_input_background}};
		border: 0;
		-webkit-border-radius: {{btn_radius}};
		-moz-border-radius: {{btn_radius}};
		border-radius: {{btn_radius}};
		color: {{newsletter_input_text}};
		margin-bottom: 0;
		padding: 0 30px;
		margin: 0;
	}
	.footer-newsletter input::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
	  color: {{newsletter_input_text}};
	}

	.footer-newsletter input:-ms-input-placeholder { /* Internet Explorer 10-11 */
	  color: {{newsletter_input_text}};
	}

	.footer-newsletter input::-ms-input-placeholder { /* Microsoft Edge */
	 color: {{newsletter_input_text}};
	}
	
	.footer-newsletter .actions{
		position: absolute;
		top: 0;
		right: 0;
	}
	.rtl-layout .footer-newsletter .actions{
		right: auto;
		left: 0;
	}
	.footer-newsletter .actions .subscribe{
		font-size: 0;
		padding: 0;
		width: 38px;
		height: 38px;
		line-height: 38px;
		border: 0;
		background: transparent;
		color: {{newsletter_button_text}};
		background: {{newsletter_button_background}};
		border: 1px solid {{newsletter_button_background}};
		-moz-transition: all 500ms ease;
		-webkit-transition: all 500ms ease;
		-o-transition: all 500ms ease;
		transition: all 500ms ease;
		-moz-backface-visibility: hidden;
		-webkit-backface-visibility: hidden;
		backface-visibility: hidden;
		/*font-size: 120%;
		font-weight: 600;
		padding: 11px 30px;
		min-height: 40px;
		*/
	}
	.footer-newsletter .actions .subscribe:hover{
		color: {{newsletter_button_hover_text}};
		background: {{newsletter_button_hover_background}};
		border-color: {{newsletter_button_hover_background}};
		-moz-transform: translateX(-5px);
		-webkit-transform: translateX(-5px);
		-o-transform: translateX(-5px);
		transform: translateX(-5px);
	}
	.footer-newsletter .actions .subscribe:before{
		display: inline-block;
		font-family: FontAwesome;
		font-style: normal;
		font-weight: normal;
