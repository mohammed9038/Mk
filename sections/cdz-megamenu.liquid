												style = "position: absolute; right: {{block.settings.mmdd_bg_position_x}}px; bottom: {{block.settings.mmdd_bg_position_y}}px;"
											{% endcase %} />
										{%- endunless -%}
										<div class="groupmenu-drop-content  {{block.settings.mmdd_width}}">
											<div class="menu-column-content">
												{{block.settings.mmdd_html_col1}}
											</div>
										</div>
									</li>
								</ul>
								{%- when 'html-two-columns' -%}
