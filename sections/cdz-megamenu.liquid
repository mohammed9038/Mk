{%- assign cdzmegamenu_wrapper_class = section.settings.cdzmegamenu_wrapper_class -%}
{%- assign cdzmegamenu_ddstyle = section.settings.cdzmegamenu_ddstyle -%}
{%- assign cdzmegamenu_ddanimation = section.settings.cdzmegamenu_ddanimation -%}
{%- assign cdzmegamenu_type = section.settings.cdzmegamenu_type -%}
{%- assign cdzmegamenu_identifier = section.settings.cdzmegamenu_identifier -%}
{%- assign cdzmegamenu_status = section.settings.cdzmegamenu_status -%}
{%- assign bpMMenu =  settings.responsive_header -%}
{%- if bpMMenu == blank -%}
{%- assign bpMMenu =  '768' -%}
{%- endif -%}
{%- if cdzmegamenu_status == "enable" -%}
<div data-section-id="{{ section.id }}" data-section-type="cdz-megamenu" id="cdzmegamenu-section-{{ section.id }}" {% unless cdzmegamenu_identifier == blank %} data-hide='menu-hidden' {% endunless %}>
<nav data-mmenu-id="{{cdzmegamenu_identifier}}" class="cdz-navigation supports-js {{cdzmegamenu_wrapper_class}}" data-action="navigation" {% if cdzmegamenu_identifier != blank %}data-cdzprewidget{% else %}data-cdzwidget{% endif%}='{"codazon.buildCdzMegaMenu" : {"type" : "{{cdzmegamenu_type}}", "animation" : "{{cdzmegamenu_ddanimation}}", "dropdownStyle" : "{{cdzmegamenu_ddstyle}}", "breakpoint" : {{bpMMenu}}{% unless section.settings.cdzmegamenu_identifier == blank %}, "identifier" : "{{ cdzmegamenu_identifier }}"{% endunless %}}}'>
	<div class="cdz-menu cdz-desk-menu {% if cdzmegamenu_type == 'horizontal' %} cdz-horizontal-menu {% elsif cdzmegamenu_type == 'vertical' %}cdz-vertical-menu {% endif %}{% if cdzmegamenu_ddanimation == 'transform' %}  cdz-transform {% endif %}">
		<ul class="groupmenu">
			{%- for block in section.blocks -%}
				{%- if forloop.length > 0 -%}
						{% comment ls-rpl %}{%- assign sizeimg_url = block.settings.cdzmegamenu_itemimgicon | img_url : block.settings.cdzmegamenu_item_imgsize -%}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: block.settings.cdzmegamenu_itemimgicon, img_url : block.settings.cdzmegamenu_item_imgsize -%}{%- endcapture -%}{%- assign sizeimg_url = LS_IMG_URL -%}{% comment /ls-rpl %}{% endcomment %}
						<li class="item level0 {% unless block.settings.cdzmegamenu_customclass == blank %}{{ block.settings.cdzmegamenu_customclass }}{% endunless %}{% unless block.type == 'menulink'%} parent{% endunless %} {% if block.settings.cdzmegamenu_itemurl contains collection.handle %}active{% endif %}">
							<a class="menu-link" href="{% if block.settings.cdzmegamenu_itemurl == blank%}javascript:void(0){% else %}{{block.settings.cdzmegamenu_itemurl}}{% endif %}">
							{%- if block.settings.cdzmegamenu_itemicon == 'mmfonticon' -%}
								{%- unless block.settings.cdzmegamenu_itemfonticon == blank -%}<span class="fa 		{{block.settings.cdzmegamenu_itemfonticon}}"></span>{%- endunless -%}
							{%- else -%}
								{%- unless block.settings.cdzmegamenu_itemimgicon == blank -%}<i class="menu-icon img-icon"><img src="{{sizeimg_url}}" /></i>{%- endunless -%}
							{%- endif -%}
							<span>{{block.settings.cdzmegamenu_title}}</span>{%- unless block.settings.cdzmegamenu_lbitem == blank -%}<span class="mm-item-label {{block.settings.cdzmegamenu_lbclscustom}}"><span class="mm-label" style="color: {{block.settings.cdzmegamenu_lbcolor}}; background-color: {{block.settings.cdzmegamenu_lbbackground}}; border-color: {{block.settings.cdzmegamenu_lbbackground}}">{{block.settings.cdzmegamenu_lbitem}}</span></span>{%- endunless -%}</a>
							{%- case block.type -%}
								{%- when 'menu' -%}
								<ul class="cat-tree groupmenu-drop groupmenu-width">
									{%- for link in linklists[block.settings.cdzmegamenu_list].links -%}
									<li class="level1 item nav-{{forloop.index}}{% if link.active %} active {% if link.child_active %}child-active{% endif %}{% endif %} {% if link.links != blank %} parent {% endif %}">
									<a class="menu-link" href="{{ link.url }}"><span>{{ link.title }}</span></a>
									{%- if link.links != blank -%}
									  <ul class="cat-tree child">
										{%- for child_link in link.links -%}  
										<li class="level2 item nav-{{forloop.index}} {% if child_link.active %} active {% if child_link.child_active %}child-active{% endif %}{% endif %}{% if child_link.links != blank %} parent {% endif %}">
										<a class="menu-link" href= "{{ child_link.url }}"><span>{{ child_link.title }}</span></a>
										{%- if child_link.links != blank -%}
										  <ul class="cat-tree child">
										  {%- for grandchild_link in child_link.links -%}  
											<li class="level3 item nav-{{forloop.index}} {% if grandchild_link.active %} active {% if grandchild_link.child_active %}child-active{% endif %}{% endif %}"
											><a class="menu-link" href= "{{ grandchild_link.url }}"><span>{{ grandchild_link.title }}</span></a></li>
										  {%- endfor -%}
										  </ul>
										{%- endif -%}   
										</li>
										{%- endfor -%}
									  </ul> 
									{%- endif -%} 
									</li>
									{%- endfor -%}
								</ul>
								{%- when 'html-onecolumn' -%}
								<ul class="groupmenu-drop">
									<li class="item level1" style="{{block.settings.mmdd_custom_css_inline}}">
										{%- unless block.settings.mmdd_bg_content == blank -%}
											<img class="image-background" src="{% comment ls-rpl %}{{block.settings.mmdd_bg_content | img_url : 'master'}}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: block.settings.mmdd_bg_content, img_url : 'master' -%}{%- endcapture -%}{{LS_IMG_URL}}{% comment /ls-rpl %}{% endcomment %}" {% case block.settings.mmdd_bg_position %}
