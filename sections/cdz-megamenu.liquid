										{%- unless block.settings.mmdd_bg_content == blank -%}
											<img class="image-background" src="{% comment ls-rpl %}{{block.settings.mmdd_bg_content | img_url : 'master'}}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: block.settings.mmdd_bg_content, img_url : 'master' -%}{%- endcapture -%}{{LS_IMG_URL}}{% comment /ls-rpl %}{% endcomment %}" {% case block.settings.mmdd_bg_position %}
											{% when '0' %}
