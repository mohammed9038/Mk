{% schema %}
{
  "name": "Codazon MegaMenu",
  "max_blocks": 12,
  "settings": [
	{
      "type": "text",
      "id": "title",
      "label": "Heading",
      "default": "Codazon MegaMenu"
    },
	{
	   "type":      "text",
	   "id":        "cdzmegamenu_identifier",
	   "label":     "Identifier"
	},
	{
	   "type":      "text",
	   "id":        "cdzmegamenu_wrapper_class",
	   "label":     "Wrapper CSS Class"
	},
	{
	   "type":      "select",
	   "id":        "cdzmegamenu_type",
	   "options": [
		  { "value": "horizontal", "label": "Horizontal"},
		  { "value": "vertical", "label": "Vertical"},
		  { "value": "toggle", "label": "Toggle"}
	   ],
	   "label":     "Type",
	   "default" : "horizontal"
	},
	{
	   "type":      "select",
	   "id":        "cdzmegamenu_ddstyle",
	   "options": [
		  { "value": "auto", "label": "Auto Width"},
		  { "value": "full", "label": "Full Width"}
	   ],
	   "label":     "Dropdown Style",
	   "default" : "auto"
	},
	{
	   "type":      "select",
	   "id":        "cdzmegamenu_ddanimation",
	   "options": [
		  { "value": "normal", "label": "Normal"},
		  { "value": "fade", "label": "Fade"},
		  { "value": "slide", "label": "Slide"},
		  { "value": "transform", "label": "Transform"}
	   ],
	   "label":     "Dropdown Animation",
	   "default" : "transform"
	},
	{
	   "type":      "select",
	   "id":        "cdzmegamenu_status",
	   "options": [
		  { "value": "enable", "label": "Enable"},
		  { "value": "disable", "label": "Disable"}
	   ],
	   "label":     "Status",
	   "default" : "enable"
	}
  ],
  "blocks": [
	{
      "type": "menulink",
      "name": "Menu Item Link",
      "settings": [
        {
          "type": "text",
          "id": "cdzmegamenu_title",
          "label": "Menu Item Title",
          "default": "Title Item"
        },
        {
          "type": "url",
          "id": "cdzmegamenu_itemurl",
          "label": "Menu Item URL"
        },
		{
			"type" : "header",
			"content" : "-- Label For Menu Item --"
		},
        {
          "type": "text",
          "id": "cdzmegamenu_lbitem",
          "label": "Menu Item Label"
        },
		{
          "type": "color",
          "id": "cdzmegamenu_lbcolor",
          "label": "Item Label Color",
		  "default": "#ffffff",
		  "info": "Default: [#ffffff]"
        },
		{
          "type": "color",
          "id": "cdzmegamenu_lbbackground",
          "label": "Item Label Background Color",
		  "default" : "#fdbc20",
		  "info": "Default: [#fdbc20]"
        },
        {
          "type": "text",
          "id": "cdzmegamenu_lbclscustom",
          "label": "Custom Class For Label Icon"
        },
		
		{
			"type" : "header",
			"content" : "-- Icon For Menu Item --"
		},
		{
		   "type":      "select",
		   "id":        "cdzmegamenu_itemicon",
		   "options": [
			  { "value": "mmfonticon", "label": "Show Font Icon"},
			  { "value": "mmimgicon", "label": "Show Image Icon"}
		   ],
		   "label":     "Show Font or Image Icon",
		   "default" : "mmfonticon"
		},
		{
			"type" : "text",
