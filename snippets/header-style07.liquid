	.page-header{
		color: {{header_text}};
		background-color: {{header_background}};
		background-position: {{header_background_image_option}};
		{% unless header_background_file == blank %}
		background-image: url({% comment ls-rpl %}{{header_background_file | img_url }}{% endcomment %}{%- capture LS_IMG_URL -%}{%- render "ls-img-url", image: header_background_file, img_url -%}{%- endcapture -%}{{LS_IMG_URL }}{% comment /ls-rpl %}{% endcomment %});
		{% endunless %}
		margin-bottom: 0;
		font: {{ header-font }};
	}
	.page-header .header-top{
		color: {{header_top_text}};
		background: {{header_top_background}};
	}
	.page-header .header-panel-bottom{
      color: {{header_search_text}};
      background: {{menu_background}};
	  border-top: 1px solid {{header_border}};
      }
	/*Toogle Left Header*/
	.page-header .utilies-toggle-wrap{
		border: 2px solid {{header_top_text}};
	}
	.page-header .utilies-toggle-wrap a:before{
		color: {{header_text}};
	}
		/*features links*/
		.page-header .features-links > li.nav.item a{
			color: {{ header_top_text }};
		}	
		.page-header .features-links > li.nav.item a:hover{
			color: {{ header_hover_text }};
		}
		 .page-header .sticky-menu:not(.active) .features-links>li{
			border-left: 1px solid {{header_border}};
		 }
		.page-header #top-wishlist,
		.page-header .showcart{			
            color: {{ header_top_text }};
		}
		.page-header #top-wishlist:hover,
		.page-header .showcart:hover{
			color: {{ header_hover_text }};
		}
		.page-header .account-trigger span.text-uppercase,
		.page-header .showcart span.text-uppercase{
            color: {{header_text}};
		}
		.page-header .account-trigger:hover > span,
		.page-header .showcart:hover > span{
			color:{{header_hover_text}} ;
		}
		/*Account*/
		.page-header .sticky-menu:not(.active) .features-links>li .text-underlink{
			border-bottom: 1px solid {{header_top_text}};	
		}
		.page-header .sticky-menu:not(.active) .features-links>li:hover .text-underlink{
			border-bottom-color: {{header_hover_text}};	
		}
		
		.page-header .sticky-menu:not(.active) .features-links>li .text-uppercase {
			font-weight: 700;	
		}
		.page-header .account-wrapper .account-trigger:hover{
			border-color:{{header_hover_text}};						
		}
		.page-header .account-wrapper .account-trigger:hover span{
			color:{{header_hover_text}};						
		}
		/*Cart*/
		.page-header .action.showcart:hover:before{
			color:{{header_hover_text}};	
		}
		.page-header .action.showcart:before{			
            color:{{header_cart_icon}};
        }
		.page-header .action.showcart .counter.qty{
			background: {{header_cart_quality_background}};
		}
		/*Whislist*/
		.page-header .wishlist .counter.qty{
			background: {{header_cart_quality_background}};
		}
		/*Search*/	
		.page-header .minisearch .actions .search{
            color: {{ header_search_button_text }};
            background: {{header_search_button_background}};
		}
		.page-header .header-search{	
			background: {{header_search_background}};
		}
		.page-header .header-search .input-text{
            color: {{ header_search_text }};
            background: {{ header_search_background }};   	
		}
		 .page-header .header-search .input-group-btn .search-button{
            color: {{ header_search_button_text }};
            background: {{ header_search_button_background }};
		}
